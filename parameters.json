            "value": [
                "192.168.0.0/24"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "Subnet1-US-Central",
                    "properties": {
                        "addressPrefix": "192.168.0.0/25"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "VM1-US-Central-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "virtualMachineName": {
            "value": "VM1-US-Central"
        },
        "virtualMachineComputerName": {
            "value": "VM1-US-Central"
        },
        "virtualMachineRG": {
            "value": "VM1-US-Central_group"
        },
        "osDiskType": {
            "value": "StandardSSD_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_DS1_v2"
        },
        "adminUsername": {
            "value": "apraveen"
        },
        "adminPassword": {
            "value": null
        },
        "patchMode": {
            "value": "AutomaticByOS"
        },
        "enableHotpatching": {
            "value": false
        }
    }
}